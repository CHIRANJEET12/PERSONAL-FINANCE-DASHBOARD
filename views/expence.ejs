<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Expense Tracker Dashboard</title>
    <link rel="stylesheet" href="path/to/your/css/styles.css">
</head>
<body>
    <div class="expense-tracker">
        <h1 class="heading">Track Your Expenses</h1>
        <ul class="expenses-container" id="expensesContainer">
            <% if (expenses.length > 0) { %>
                <% expenses.forEach(function(expense) { %>
                    <li class="expenseCard">
                        <p>Money: <%= expense.money %></p>
                        <p>Description: <%= expense.desc %></p>
                        <p>Date: <%= new Date(expense.createdAt).toLocaleDateString() %></p>
                        <p>Time: <%= new Date(expense.createdAt).toLocaleTimeString() %></p>
                    </li>
                <% }); %>
            <% } else { %>
                <p id="no-cards-msg">No expenses found. To add an expense, click on the add icon.</p>
            <% } %>
        </ul>
        <div class="addIcon">
            <span id="add-expense" class="material-symbols-outlined plusIcon">add</span>
        </div>
    </div>

    <div class="goals">
        <h1 class="goalHeading">Dream Big Save Now</h1>
        <ul id="goal-items"></ul>
        <div class="addIcon">
            <span id="add-goal" class="material-symbols-outlined plusIcon">add</span>
        </div>
    </div>

    <script src="path/to/your/js/script.js"></script>
</body>
</html>
